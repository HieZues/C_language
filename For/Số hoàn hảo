#include <stdio.h>

int main (){
   int i, n, TongUoc = 0;
   printf ("Nhap so nguyen duong N: ");
   scanf ("%d", &n);

   for (i = 1; i < n; i++) {
       if (n % i == 0) {
         TongUoc += i;
       }
   }
   if (TongUoc == n) {
    printf ("%d la so Hoan Hao", n);
   }
   else {
    printf ("%d khong phai la so Hoan Hao", n);
   }
   return 0;
}
