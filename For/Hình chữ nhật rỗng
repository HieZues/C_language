#include <stdio.h>

int main (){
  int i, j, n, m;
  printf ("Nhap chieu dai cua hinh chu nhat: "); scanf ("%d", &n);
  printf ("Nhap chieu rong cua hinh chu nhat: "); scanf ("%d", &m);


  for (i = 1; i <= m; i ++){

    for (j = 1; j <= n; j ++) {
      if (i == 1 || i == m || j == 1 || j == n) {
        printf (" * ");
      }
              else {
            printf ("   ");
        }

  }
  printf ("\n");
}

}
