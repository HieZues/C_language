#include <stdio.h>

int main (){
   int n, i, dem = 0;
   printf ("Nhap N: ");
   scanf ("%d", &n);

   if (n < 2 ){
    printf ("%d khong phai la so nguyen to\n", n);
   }
   else {
    for (i = 2; i <= n - 1 ; i++){
        if ( n % i == 0){
            dem++;
            break;
        }
    }
    if (dem++ == 0){
        printf ("%d la so nguyen to", n);
    }
    else {
        printf ("%d khong phai la so nguyen to", n);
    }
   }

   }

